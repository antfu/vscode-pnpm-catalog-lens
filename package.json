{
  "publisher": "antfu",
  "name": "pnpm-catalog-lens",
  "displayName": "PNPM Catalog Lens",
  "version": "0.3.5",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "description": "Show inlay version for PNPM catalogs",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/vscode-pnpm-catalog-lens#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/vscode-pnpm-catalog-lens"
  },
  "bugs": {
    "url": "https://github.com/antfu/vscode-pnpm-catalog-lens/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/antfu"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.90.0"
  },
  "activationEvents": [
    "workspaceContains:pnpm-workspace.yaml"
  ],
  "contributes": {
    "commands": [
      {
        "category": "PNPM Catalog Lens",
        "title": "Toggle",
        "command": "pnpmCatalogLens.toggle"
      },
      {
        "category": "PNPM Catalog Lens",
        "title": "Go to Definition",
        "command": "pnpmCatalogLens.gotoDefinition"
      }
    ],
    "configuration": {
      "type": "object",
      "properties": {
        "pnpmCatalogLens.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable inlay hints"
        },
        "pnpmCatalogLens.hover": {
          "type": "boolean",
          "default": true,
          "description": "Show dependency info on hover"
        },
        "pnpmCatalogLens.namedCatalogsColors": {
          "type": "boolean",
          "default": true,
          "description": "Give each named catalog a unique color"
        },
        "pnpmCatalogLens.namedCatalogsColorsSalt": {
          "type": "string",
          "default": "pnpm-catalog-lens",
          "description": "A random string to adding as the salt for the named catalogs colors"
        },
        "pnpmCatalogLens.namedCatalogsLabel": {
          "type": "boolean",
          "default": true,
          "description": "Show a small label for named catalog in the inlay hint"
        }
      }
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --external vscode --format cjs",
    "dev": "nr build --watch",
    "prepare": "nr update",
    "update": "vscode-ext-gen --output src/generated/meta.ts --scope pnpmCatalogLens",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce pack --no-dependencies",
    "package": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.1.0",
    "@antfu/ni": "^25.0.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/traverse": "^7.28.0",
    "@babel/types": "^7.28.2",
    "@reactive-vscode/vueuse": "^0.3.1",
    "@types/babel__core": "^7.20.5",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.2.0",
    "@types/vscode": "1.90.0",
    "@vscode/vsce": "^3.6.0",
    "bumpp": "^10.2.2",
    "eslint": "^9.32.0",
    "eslint-plugin-format": "^1.0.1",
    "find-up": "^7.0.0",
    "js-yaml": "^4.1.0",
    "pnpm": "^10.14.0",
    "reactive-vscode": "^0.3.1",
    "rolldown": "1.0.0-beta.31",
    "tsdown": "^0.13.3",
    "typescript": "^5.8.2",
    "vite": "^7.0.6",
    "vitest": "^3.2.4",
    "vscode-ext-gen": "^1.1.0",
    "yaml-eslint-parser": "^1.3.0"
  }
}
